{
    "$id": "http://radicalvpn.com/schemas/vpn/create",
    "type": "object",
    "properties": {
        "alias": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30,
            "pattern": "^[a-zA-Z1-9 ]+$"
        },
        "node": {
            "type": "string",
            "format": "uuid"
        }
    },
    "required": ["alias", "node"],
    "additionalProperties": false
}
